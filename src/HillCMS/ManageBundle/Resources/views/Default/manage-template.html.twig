            <script src="../assets/js/html5shiv.js"></script>
        <![endif]-->
<link rel="shortcut icon" href={{ asset("includes/images/manageicon.png")}}>
<head>
{% block header %}
{% endblock %}
</head>

<body>
{% block body %}
	<div class="tabbable tabs-left">
	{% block navmenu %}
	{# Expects some set of pages to be read from the database that a user has permission to. #}
		
		<ul class="nav nav-tabs span2" style="min-height:100%;">
		<div class = "nav-header">
			Editable Pages
		</div>
		{% block pagemenu %}
				<!-- Temporary placeholder until pages get loaded... example -->
				<li class="active"><a href="#lF" data-toggle="tab">Section 1</a></li>
                <li class=""><a href="#lB" data-toggle="tab">Section 2</a></li>
                <li class=""><a href="#lC" data-toggle="tab">Section 3</a></li>
		{% endblock %}
        </ul>
        <div class="tab-content span8">
        	{% block pagethings %}
        	<div class="tab-pane" id="lF">
            	<p>I'm in Section A.</p>
            </div>
            <div class="tab-pane active" id="lB">
                <p>Howdy, I'm in Section B.</p>
            </div>
            <div class="tab-pane" id="lC">
            	<p>What up girl, this is Section C.</p>
            </div>
            {% endblock pagethings %}
        </div>
	{% endblock %}
	</div>
{% endblock %}
</body>
{% block footer %}
{% endblock %}
<!-- /.container -->
<!-- Placed at the end of the document so the pages load faster -->
<script
	src="http://ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
